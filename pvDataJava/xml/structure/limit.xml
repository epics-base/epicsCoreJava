<?xml version="1.0" ?>
<database xmlns="urn:schemas-epics-org:javaioc:1.0"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="urn:schemas-epics-org:javaioc:1.0 javaioc.xsd"
   xmlns:model="urn:schemas-cosylab-com:model:1.0"
   name="database">>

<structure structureName = "byteLimit">
    <scalar name = "low" scalarType = "byte" />
    <scalar name = "high" scalarType = "byte" />
</structure>

<structure structureName = "shortLimit">
    <scalar name = "low" scalarType = "short" />
    <scalar name = "high" scalarType = "short" />
</structure>

<structure structureName = "intLimit">
    <scalar name = "low" scalarType = "int" />
    <scalar name = "high" scalarType = "int" />
</structure>

<structure structureName = "longLimit">
    <scalar name = "low" scalarType = "long" />
    <scalar name = "high" scalarType = "long" />
</structure>

<structure structureName = "floatLimit">
    <scalar name = "low" scalarType = "float" />
    <scalar name = "high" scalarType = "float" />
</structure>

<structure structureName = "doubleLimit">
    <scalar name = "low" scalarType = "double" />
    <scalar name = "high" scalarType = "double" />
</structure>

</database>
