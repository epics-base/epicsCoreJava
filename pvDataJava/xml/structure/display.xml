<?xml version="1.0" ?>
<database xmlns="urn:schemas-epics-org:javaioc:1.0"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="urn:schemas-epics-org:javaioc:1.0 javaioc.xsd"
   xmlns:model="urn:schemas-cosylab-com:model:1.0"
   name="database">>

<structure structureName = "display">
    <scalar name = "description" scalarType = "string" />
    <scalar name = "format" scalarType = "string" />
    <scalar name = "units" scalarType = "string" />
    <scalar name = "resolution" scalarType = "int" />
    <structure name = "limit" extends = "doubleLimit" />
</structure>

</database>
