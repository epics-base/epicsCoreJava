<?xml version="1.0" ?>
<database>

<structure structureName = "powerFromPowerSupply">
    <scalar name = "fieldList" scalarType = "string">
       alarm,timeStamp,power.value
    </scalar>
</structure>

<structure structureName = "powerSupplyFromPowerSupply">
    <scalar name = "alarm" scalarType = "string">alarm</scalar>
    <scalar name = "timeStamp" scalarType = "string">timeStamp</scalar>
    <structure name = "power">
        <scalar name = "fieldList" scalarType = "string">power.value,power.alarm</scalar>
    </structure>
    <structure name = "current">
        <scalar name = "fieldList" scalarType = "string">current.value,current.alarm</scalar>
    </structure>
    <structure name = "voltage">
        <scalar name = "fieldList" scalarType = "string">voltage.value,voltage.alarm</scalar>
    </structure>
</structure>

<structure structureName = "powerSupplyFromPowerSupplyArray">
    <scalar name = "alarm" scalarType = "string">alarm</scalar>
    <scalar name = "timeStamp" scalarType = "string">timeStamp</scalar>
    <structure name = "power">
        <scalar name = "fieldList" scalarType = "string">supply.0.power.value,supply.0.power.alarm</scalar>
    </structure>
    <structure name = "current">
        <scalar name = "fieldList" scalarType = "string">supply.0.current.value,supply.0.current.alarm</scalar>
    </structure>
    <structure name = "voltage">
        <scalar name = "fieldList" scalarType = "string">supply.0.voltage.value,supply.0.voltage.alarm</scalar>
    </structure>
</structure>


</database>

